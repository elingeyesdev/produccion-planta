{
    "name": "lukehowland/helpdeskwidget",
    "description": "Widget embebible para integrar Helpdesk en proyectos Laravel externos",
    "version": "2.0.0",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "helpdesk",
        "widget",
        "tickets",
        "support",
        "adminlte"
    ],
    "authors": [
        {
            "name": "Lucas De La Quintana Montenegro",
            "email": "lukqs05@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/console": "^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Lukehowland\\HelpdeskWidget\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lukehowland\\HelpdeskWidget\\HelpdeskWidgetServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}